require "json"

module JParse 
    class J 
        def preproc
            date_post = Time.local
            json = File.open("Modules/Server/Settings/Server.json") do |file|
                a = JSON.parse(file)
                dt = a["Port"].to_s
                key = a["Membership_Type"].to_s
                puts "\e[38;5;49m[\e[38;5;55mINFOR\e[38;5;49m]\e[38;5;49m[\e[38;5;55m#{date_post}\e[38;5;49m][\e[38;5;39mServerRt\e[38;5;49m] \e[38;5;55m| \e[38;5;20mServer running on -> \e[38;5;39m#{a["URL"]}"
                if key == "BASIC_NO_KEY"
                    puts "\n\e[38;5;49m[\e[38;5;55mIMPORTANT\e[38;5;49m]\e[38;5;49m[\e[38;5;55m#{date_post}\e[38;5;49m][Membership Information] \n\e[40;6;193m This is a beta version of Frizz, this also means that your key is not a key and is not assigned to the professional version. In order to get access to the professional version of frizz packet capturing you must purchase a valid license. This is just a warning that you will not have all features of the frizz project, this also means you are a beta tester and bugs may occure. Thank you for trying out and choosing frizz as a tool for packet capturing!\e[40;6;0m\n"
                end
                dt.to_i

            end
        end
    end
end